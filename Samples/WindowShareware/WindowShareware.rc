1 MENU
{
    POPUP "&WindowShareware"
    {
        MENUITEM "&Addition", ID_ADD
        MENUITEM "&Subtraction", ID_SUB
        MENUITEM "&Multiplication (registration required)", ID_MUL
        MENUITEM "&Division (registration required)", ID_DIV
        MENUITEM SEPARATOR
        MENUITEM "&Exit\tAlt+F4", ID_EXIT
    }
}

